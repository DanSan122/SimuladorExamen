{
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/preguntas.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "@vercel/node"
    }
  }
}
